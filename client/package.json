{
	"name": "ham-san-net",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "svelte-kit sync",
		"start": "node --experimental-loader ./scripts/loader.js -r dotenv/config build",
		"codegen": "graphql-codegen",
		"codegen:prod": "graphql-codegen -c codegen.prod.yml",
		"build:prod": "pnpm codegen:prod && pnpm build"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^3.2.1",
		"@graphql-codegen/typed-document-node": "^3.0.1",
		"@graphql-codegen/typescript": "^3.0.1",
		"@graphql-codegen/typescript-generic-sdk": "^3.1.0",
		"@graphql-codegen/typescript-graphql-request": "^4.5.8",
		"@graphql-codegen/typescript-operations": "^3.0.1",
		"@playwright/test": "^1.31.1",
		"@sveltejs/adapter-auto": "^1.0.3",
		"@sveltejs/adapter-node": "^1.2.0",
		"@sveltejs/kit": "^1.8.3",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"graphql-codegen-svelte-apollo": "^1.1.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"sass": "^1.58.3",
		"svelte": "^3.55.1",
		"svelte-carousel": "^1.0.23",
		"svelte-check": "^3.0.4",
		"svelte-preprocess": "^5.0.1",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.4",
		"vite-imagetools": "^4.0.18",
		"vitest": "^0.25.8"
	},
	"type": "module",
	"dependencies": {
		"@apollo/client": "^3.7.9",
		"@fortawesome/free-brands-svg-icons": "^6.3.0",
		"@fortawesome/free-solid-svg-icons": "^6.3.0",
		"date-fns": "^2.29.3",
		"dotenv": "^16.0.3",
		"fast-sha256": "^1.3.0",
		"graphql": "^16.6.0",
		"graphql-request": "^5.1.0",
		"graphql-tag": "^2.12.6",
		"lodash": "^4.17.21",
		"svelte-apollo-client": "^0.1.4",
		"svelte-fa": "^3.0.3",
		"svelte-markdown": "^0.2.3",
		"sveltekit-i18n": "^2.2.2"
	}
}
