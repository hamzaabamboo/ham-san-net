{
	"name": "client",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"serve": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"sync": "svelte-kit sync",
		"start": "node --experimental-loader ./scripts/loader.js -r dotenv/config build",
		"codegen": "graphql-codegen",
		"codegen:prod": "graphql-codegen -c codegen.prod.yml",
		"build:prod": "pnpm codegen:prod && pnpm build"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^3.3.1",
		"@graphql-codegen/typescript-graphql-request": "^4.5.9",
		"@graphql-codegen/typescript-operations": "^3.0.4",
		"@playwright/test": "^1.39.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.25.2",
		"@types/three": "^0.156.0",
		"@typescript-eslint/eslint-plugin": "^6.7.5",
		"@typescript-eslint/parser": "^6.7.5",
		"@vite-pwa/sveltekit": "^0.2.7",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.51.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"graphql-codegen-svelte-apollo": "^1.1.0",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"rollup-plugin-visualizer": "^5.9.2",
		"sass": "^1.69.3",
		"svelte": "^4.2.1",
		"svelte-carousel": "^1.0.25",
		"svelte-check": "^3.5.2",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.11",
		"vite-imagetools": "^5.1.2",
		"vitest": "^0.34.6"
	},
	"type": "module",
	"dependencies": {
		"@apollo/client": "^3.8.5",
		"@fortawesome/free-brands-svg-icons": "^6.4.2",
		"@fortawesome/free-solid-svg-icons": "^6.4.2",
		"@types/remove-markdown": "^0.3.2",
		"ammo.js": "^0.0.10",
		"ammojs3": "^0.0.8",
		"date-fns": "^2.30.0",
		"dotenv": "^16.3.1",
		"fets": "^0.4.15",
		"graphql": "^16.8.1",
		"graphql-request": "5.1.0",
		"graphql-tag": "^2.12.6",
		"lodash": "^4.17.21",
		"remove-markdown": "^0.5.0",
		"svelte-fa": "^3.0.4",
		"svelte-inview": "^4.0.1",
		"svelte-markdown": "^0.4.0",
		"sveltekit-i18n": "^2.4.2",
		"three": "^0.157.0",
		"vite-plugin-pwa": "^0.16.5"
	}
}
