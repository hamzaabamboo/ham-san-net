{
  "name": "client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "eslint .",
    "sync": "svelte-kit sync",
    "start": "node --experimental-loader ./scripts/loader.js -r dotenv/config ../../dist/apps/client/index.js",
    "start:prod": "node --experimental-loader ./scripts/loader.js -r dotenv/config index.js",
    "codegen": "graphql-codegen",
    "codegen:prod": "graphql-codegen -c codegen.prod.yml",
    "build:prod": "pnpm codegen:prod && pnpm build"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@playwright/test": "^1.42.1",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/kit": "^2.5.5",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/three": "^0.163.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vite-pwa/sveltekit": "^0.4.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "graphql-codegen-svelte-apollo": "^1.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.72.0",
    "svelte": "^4.2.12",
    "svelte-carousel": "^1.0.25",
    "svelte-check": "^3.6.8",
    "svelte-preprocess": "^5.1.3",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.7",
    "vite-imagetools": "^7.0.1",
    "vitest": "^1.4.0"
  },
  "type": "module",
  "dependencies": {
    "@apollo/client": "^3.9.9",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@types/remove-markdown": "^0.3.4",
    "ammo.js": "^0.0.10",
    "ammojs3": "^0.0.11",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "fets": "^0.8.0",
    "graphql": "^16.8.1",
    "graphql-request": "6.1.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "remove-markdown": "^0.5.0",
    "resolve": "^1.22.8",
    "svelte-fa": "^4.0.2",
    "svelte-inview": "^4.0.2",
    "svelte-markdown": "^0.4.1",
    "sveltekit-i18n": "^2.4.2",
    "three": "^0.163.0",
    "vite-plugin-pwa": "^0.19.7"
