{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": "src",
    "jsxImportSource": "react",
    "paths": {
      "~/*": ["./*"],
      "i18n/*": ["../../../libs/i18n/*"],
      "styled-system/*": ["../styled-system/*"]
    },
    "verbatimModuleSyntax": false
  },
  "include": ["src", "styled-system"]
}
